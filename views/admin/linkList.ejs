<% include ./header %>

<!-- 路径导航 -->
<ol class="breadcrumb">
    <li>
        <a href="#">友链管理</a>
    </li>
    <li class="active">友链列表</li>
</ol>
<!-- 路径导航 END -->


    <table class="table table-hover">
        <tr>
            <td>友链名称</td>
            <td>友链地址</td>
            <td>操作</td>
        </tr>
        <% datalist.forEach(function(val){ %>
            <tr>
                <td><%=val.name%></td>
                <td><%=val.url%></td>
                <td>
                    <a href="/admin/linkEdit/<%=val._id%>">编辑</a>
                    |
                    <a href="/admin/linkRemove/<%=val._id%>" onclick="return confirm('您确定要删除吗？')">删除</a>
                </td>
            </tr>
        <% })%>
    </table>
<% include ./footer %>
